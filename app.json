{
  "expo": {
    "name": "Instagramapp",
    "slug": "Instagramapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins":[
      "@react-native-google-singin/google-singin"
    ],
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.Instagramapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Instagramapp"    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "010e021f-1ac9-4665-82ba-bc705fe96e37"
      }
    }
  }
}
